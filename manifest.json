{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Dex for Chrome - Personal CRM and Rolodex",
  "description": "Be better at relationships with Dex. Visualize, understand, and manage your relationships for free.",
  "icons": {
    "16": "assets/dex-icon.png",
    "48": "assets/dex-icon.png",
    "128": "assets/dex-icon.png"
  },
  "homepage_url": "http://getdex.com",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "128": "assets/dex-icon.png"
    },
    "default_title": "Open Dex"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.linkedin.com/*",
        "*://*.facebook.com/*",
        "*://*.instagram.com/*",
        "*://*.getdex.com/*",
        "*://*.twitter.com/*",
        "*://*.x.com/*",
        "*://*.mail.google.com/*",
        "*://*.superhuman.com/*",
        "*://*.live.com/*",
        "*://*.outlook.com/*",
        "*://*.office.com/*",
        "*://*.google.com/*"
      ],
      "run_at": "document_end",
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "cookies",
    "scripting"
  ],
  "host_permissions": [
    "*://*.linkedin.com/*",
    "*://*.facebook.com/*",
    "*://*.instagram.com/*",
    "*://*.getdex.com/*",
    "*://*.twitter.com/*",
    "*://*.x.com/*",
    "*://*.mail.google.com/*",
    "*://*.superhuman.com/*",
    "*://*.live.com/*",
    "*://*.outlook.com/*",
    "*://*.office.com/*",
    "*://*.google.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "version": "2.0.7"
}